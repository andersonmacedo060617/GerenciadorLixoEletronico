@model GerenciadorLixoEletronico.NH.Model.Cidade

@{
    ViewBag.Title = "Visualizar";
    Layout = "~/Views/Shared/_AdministradorLayout.cshtml";
}

<h2>Visualizar</h2>

<div>
    <h4>Cidade</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nome)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nome)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Estado)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado.Nome)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Estado.Pais)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado.Pais.Nome)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LocalizacaoMap)
        </dt>
        <dd>
            @Html.Raw(@Model.LocalizacaoMap)
        </dd>




</dl>
    
</div>
<p>
    @Html.ActionLink("Alterar", "Alterar", new { id = Model.Id }) |
    @Html.ActionLink("Cancelar", "Index")
</p>
